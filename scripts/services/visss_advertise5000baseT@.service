# https://bbs.archlinux.org/viewtopic.php?id=262075

[Unit]
Description=ethtool configuration to enable 5000mbps speed for the  card %i 
After=network-online.target
Wants=network-online.target

[Service]
EnvironmentFile=/home/visss/VISSS_INTERFACES.env
ExecStart=/usr/sbin/ethtool -s $%i advertise 0x1000000000000
Type=oneshot

[Install]
WantedBy=multi-user.target